(define (cons x y)
	(lambda (getter) (getter x y)))

(define (car list)
	(list (lambda (x y) x)))

(define (cdr list)
	(list (lambda (x y) y)))

(define (main)	; 整体测试代码，求斐波那契数列
	(define max 10)
	(define (iter n r0 r1)
		(if (= n max) r0
			(iter (+ n 1) r1 (+ r0 r1))))
	(iter 0 0 1))
